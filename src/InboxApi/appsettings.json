{
  "AllowedHosts": "*",
  "AppSettings": {
    "Authority": "https://masked-emails-identity.azurewebsites.net",
    "RequireHttpsMetadata": true,
    "Audience": "inbox"
  },
  "Inbox":{
    "Root": "/var/mail/masked-emails.me"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
